usuario = input('digite um nome de usuario: ')
senha = input('digite uma senha: ')
while usuario == senha:
    print('Nome de usuario nao pode ser igual a senha! Digite novamente!')
    usuario = input('digite um nome de usuario: ')
    senha = input('digite uma senha: ')
    if usuario != senha:
        break
print('Cadastro efetuado com sucesso!')
print('Nome de usuario:', usuario)
print('Senha:', senha)
